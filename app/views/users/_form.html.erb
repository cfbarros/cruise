<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4">
      <div class="breath">
        <h1 class="text-center">Add a Bike</h1>
        <%= simple_form_for(@user) do |f| %>


          <%= f.input :first_name %>
          <%= f.input :address %>
          <%= f.input :last_name %>

          <%= f.submit %>

        <% end %>
      </div>
    </div>
  </div>
</div>
